{
  "version": 2,
  "buildCommand": "next build",
  "functions": {
    "src/app/api/canvas-projects/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/canvas/doors-windows/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/canvas/exclusion-zones/route.ts": {
      "maxDuration": 300
    }
  },
  "regions": ["sin1"],
  "headers": [
    {
      "source": "/api/canvas/doors-windows",
      "headers": [
        { "key": "Connection", "value": "keep-alive" },
        { "key": "Keep-Alive", "value": "timeout=120" }
      ]
    },
    {
      "source": "/api/canvas/exclusion-zones",
      "headers": [
        { "key": "Connection", "value": "keep-alive" },
        { "key": "Keep-Alive", "value": "timeout=300" }
      ]
    }
  ]
}
